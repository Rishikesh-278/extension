{
    "name": "AI QR codes",
    "version": "1.0.0",
    "description": "Create beautiful AI generated QR codes.",
    "manifest_version": 3,
    "author": "Rishikesh Bidkar",
    "action": {
        "default_popup": "popup.html",
        "default_title": "Create AI generated QR codes."
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "permissions": [
        "activeTab",
        "contextMenus"
    ]
}